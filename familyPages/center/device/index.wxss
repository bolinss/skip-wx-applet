/**index.wxss**/
page {
  height: 100%;
}
.page-wrap {
  display: flex;
  flex-direction: column;
}
.page-wrap .tabs-wrapper {
  display: flex;
}
.page-wrap .tabs-wrapper .tabs {
  width: 646rpx;
}
.page-wrap .tabs-wrapper .edit {
  display: flex;
  align-items: center;
  width: 100rpx;
  justify-content: center;
  background-color: #fff;
  font-size: 24rpx;
}
.page-wrap .collapse .van-collapse-item__title--expanded .van-cell__right-icon-wrap {
  color: #498cff;
}
.page-wrap .collapse .van-collapse-item {
  margin: 24rpx;
}
.page-wrap .collapse .van-collapse-item .van-cell {
  border-radius: 8rpx;
  padding: 29rpx 24rpx;
}
.page-wrap .collapse .van-collapse-item .van-cell .van-cell__title {
  font-size: 32rpx;
  font-weight: 400;
  color: #333333;
}
.page-wrap .collapse .userinfo .item {
  font-size: 28rpx;
  margin-bottom: 30rpx;
}
.page-wrap .collapse .userinfo .item .value {
  font-weight: 400;
  color: #333333;
  margin: 0 24rpx;
}
.page-wrap .collapse .userinfo .item .change {
  color: #498cff;
}
.page-wrap .collapse .collapse-title {
  display: flex;
  align-items: center;
}
.page-wrap .collapse .collapse-title .icon {
  margin-right: 14rpx;
  width: 24rpx;
  height: 24rpx;
}
.page-wrap .deview-list .item {
  padding: 24rpx;
  background: #f6f9ff;
  border-radius: 8px;
  display: flex;
  align-items: flex-end;
  margin-bottom: 24rpx;
}
.page-wrap .deview-list .item .info {
  flex: 1;
}
.page-wrap .deview-list .item .info .title {
  font-size: 30rpx;
  font-weight: 500;
  color: #333333;
  display: flex;
  justify-content: space-between;
}
.page-wrap .deview-list .item .info .field {
  font-size: 26rpx;
  display: flex;
  margin-top: 24rpx;
}
.page-wrap .deview-list .item .info .field .label {
  color: #999999;
  margin-right: 24rpx;
}
.page-wrap .deview-list .item .info .field .txt {
  color: #333333;
}
.page-wrap .deview-list .item .info .field .status .van-tag {
  padding: 2rpx 10rpx;
  border-radius: 14rpx;
}
.page-wrap .deview-list .item .info .field .status .van-tag--success {
  background: #a9ead3;
  color: #47d99b;
}
.page-wrap .deview-list .item .info .field .status .van-tag--danger {
  background: #f4dfe8;
  color: #ed596b;
}
.page-wrap .v1-wrap {
  display: flex;
}
.page-wrap .v1-wrap .item {
  flex: 1;
  text-align: left;
}
.page-wrap .v1-wrap .item:first-child {
  text-align: left;
}
.page-wrap .v1-wrap .item:first-child .num {
  justify-content: flex-start;
}
.page-wrap .v1-wrap .item .title {
  font-size: 22rpx;
  color: #666666;
  line-height: 30px;
}
.page-wrap .v1-wrap .item .num {
  display: flex;
  align-items: flex-end;
  justify-content: flex-start;
}
.page-wrap .v1-wrap .item .num .txt {
  font-size: 3r6px;
  font-weight: 700;
  color: #333333;
}
.page-wrap .v1-wrap .item .num .unit {
  font-size: 20rpx;
  font-weight: 500;
  color: #999999;
}
.page-wrap .v1-table {
  margin-top: 24rpx;
  border-top: 1px solid #ededed;
}
.page-wrap .v1-table .th {
  display: flex;
  padding: 24rpx 0;
}
.page-wrap .v1-table .th .name {
  font-size: 20rpx;
  flex: 1;
  font-weight: 400;
  color: #999999;
}
.page-wrap .v1-table .th .name:last-child,
.page-wrap .v1-table .th .name.time,
.page-wrap .v1-table .th .name.point {
  text-align: center;
}
.page-wrap .v1-table .th .name:last-child {
  text-align: right;
}
.page-wrap .v1-table .td {
  display: flex;
  padding: 24rpx 0;
}
.page-wrap .v1-table .td .value {
  font-size: 24rpx;
  flex: 1;
  font-weight: 400;
  color: #333333;
}
.page-wrap .v1-table .td .value:last-child,
.page-wrap .v1-table .td .value.time,
.page-wrap .v1-table .td .value.point {
  text-align: center;
}
.page-wrap .v1-table .td .value:last-child {
  text-align: right;
}
.page-wrap .v1-table .td .value .self {
  color: #498cff;
}
.page-wrap .v1-table .td .value .other {
  color: #333333;
}
.page-wrap .v1-table .td .value.time {
  color: #9d9d9d;
}
.page-wrap .v1-table .td .value.SUCCESS {
  color: #47d99b;
}
.page-wrap .honur-table {
  margin-top: 24rpx;
  border-top: 1px solid #ededed;
}
.page-wrap .honur-table .th {
  display: flex;
  padding: 24rpx 0;
}
.page-wrap .honur-table .th .name {
  font-size: 20rpx;
  flex: 1;
  font-weight: 400;
  color: #999999;
  text-align: center;
}
.page-wrap .honur-table .th .name:first-child {
  text-align: left;
}
.page-wrap .honur-table .th .name:last-child {
  text-align: right;
}
.page-wrap .honur-table .td {
  display: flex;
  padding: 24rpx 0;
}
.page-wrap .honur-table .td .value {
  font-size: 24rpx;
  flex: 1;
  font-weight: 400;
  color: #333333;
  text-align: center;
}
.page-wrap .honur-table .td .value:first-child {
  text-align: left;
}
.page-wrap .honur-table .td .value.time {
  color: #999999;
  text-align: right;
}
.page-wrap .honur-table .td .value.rank-1 {
  color: #f1914a;
}
.page-wrap .honur-table .td .value.rank-2 {
  color: #79abff;
}
.page-wrap .honur-table .td .value.rank-3 {
  color: #47d99b;
}
